<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>resourcesPlatform</display-name>
  <welcome-file-list>
    <welcome-file>homePage/home.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <display-name>EncodingFilter</display-name>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.googosoft.commons.EncodingFilter</filter-class>
    <init-param>
      <param-name>ENCODING</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>SessionFilter</display-name>
    <filter-name>SessionFilter</filter-name>
    <filter-class>com.googosoft.commons.SessionFilter</filter-class>
    <init-param>
      <param-name>ERROR</param-name>
      <param-value>/ggs/view/share/session.jsp</param-value>
    </init-param>
    <init-param>
      <param-name>SUCCESSFILES</param-name>
      <param-value>/ggs/view/share/session.jsp,/ggs/view/share/error.jsp</param-value>
    </init-param>
    <init-param>
      <param-name>SUCCESSFOLDERS</param-name>
      <param-value>/jsp,/homePage,/filesmanager,/nextIndex,/journal,/openClass,/subjectResearch,/subjectAchievement</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>uploadMovie</servlet-name>
    <servlet-class>com.googosoft.filesManager.viewManager.UploadMovieServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadMovie</servlet-name>
    <url-pattern>/uploadMovie.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>upMovie</servlet-name>
    <servlet-class>com.googosoft.filesManager.course.UploadMovieServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upMovie</servlet-name>
    <url-pattern>/upMovie</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>ViewVideo</display-name>
    <servlet-name>ViewVideo</servlet-name>
    <servlet-class>com.googosoft.filesManager.viewManager.ViewVideo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewVideo</servlet-name>
    <url-pattern>/ViewVideo/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>UploadServlet</display-name>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>com.googosoft.commons.UploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/UploadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>DownLoadServlet</display-name>
    <servlet-name>DownLoadServlet</servlet-name>
    <servlet-class>com.googosoft.commons.DownLoadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownLoadServlet</servlet-name>
    <url-pattern>/DownLoadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>DownLoad</description>
    <servlet-name>DownLoad</servlet-name>
    <servlet-class>com.googosoft.filesManager.fileManager.DownLoad</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownLoad</servlet-name>
    <url-pattern>/DownLoad/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>CDownLoad</description>
    <servlet-name>CDownLoad</servlet-name>
    <servlet-class>com.googosoft.filesManager.course.DownLoad</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CDownLoad</servlet-name>
    <url-pattern>/CDownLoad/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UpAnnouncementAttach</servlet-name>
    <servlet-class>com.googosoft.commons.UpAnnouncementAttach</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpAnnouncementAttach</servlet-name>
    <url-pattern>/UpAnnouncementAttach</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UploadEmailAttach</servlet-name>
    <servlet-class>com.googosoft.commons.UploadEmailAttach</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadEmailAttach</servlet-name>
    <url-pattern>/UploadEmailAttach</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>PreviewFilesServlet</display-name>
    <servlet-name>PreviewFilesServlet</servlet-name>
    <servlet-class>com.googosoft.convert2swf.preview.PreviewFilesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PreviewFilesServlet</servlet-name>
    <url-pattern>/PreviewFilesServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FileDelete</display-name>
    <servlet-name>FileDelete</servlet-name>
    <servlet-class>com.googosoft.filesManager.fileManager.FileDelete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileDelete</servlet-name>
    <url-pattern>/FileDelete</url-pattern>
  </servlet-mapping>
</web-app>