*{
	margin:0; padding:0;
	border:0px;
	font-size:12px;
}
html,body,form
{
    height:100%;
    width:100%;
    overflow:hidden;
}
a{
	text-decoration:none;
	color:#000;
}
div.div_body{
    height:100%;
    width:100%;
    overflow:hidden;
    padding-top:3px;
}
div#div_tree{
    height:99%;
    width:19%;
    overflow:auto;
    float:left;
    border: 1px solid #A6C2D7;
    margin-left:0.1%;
}
div#leftTree{
    height:100%;
    width:100%;
}
div#div_list{
    height:99%;
    width:80%;
    float:left;
    overflow:hidden;
    border: 1px solid #A6C2D7;
    margin-left:0.3%;
}
div.clear{ clear:both;}